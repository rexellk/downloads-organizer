{
  "serviceName": "downloads-organizer-backend",
  "cluster": "downloads-organizer-cluster",
  "taskDefinition": "downloads-organizer-backend",
  "desiredCount": 1,
  "launchType": "FARGATE",
  "platformVersion": "LATEST",
  "networkConfiguration": {
    "awsvpcConfiguration": {
      "subnets": [
        "subnet-YOUR_SUBNET_ID_1",
        "subnet-YOUR_SUBNET_ID_2"
      ],
      "securityGroups": [
        "sg-YOUR_SECURITY_GROUP_ID"
      ],
      "assignPublicIp": "ENABLED"
    }
  },
  "loadBalancers": [
    {
      "targetGroupArn": "arn:aws:elasticloadbalancing:YOUR_REGION:YOUR_ACCOUNT_ID:targetgroup/downloads-organizer-tg/YOUR_TARGET_GROUP_ID",
      "containerName": "downloads-organizer-backend",
      "containerPort": 8000
    }
  ],
  "deploymentConfiguration": {
    "maximumPercent": 200,
    "minimumHealthyPercent": 50
  }
}